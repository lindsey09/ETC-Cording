//배열
const array = [1, "eifj", 3, {}, 5];
console.log(array[3].name); //undefind

const object = [{ name: "멍멍" }, { name: "멍야옹" }, { name: "멍멍이이" }];
console.log(object[1].name); //멍야옹

object.push({name: "냥냥"}); //배열에 추가하기
console.log(object);

//배열의 크기를 알아내려면 length 몇개가 들어있는지 알려줌
console.log(object.length);
